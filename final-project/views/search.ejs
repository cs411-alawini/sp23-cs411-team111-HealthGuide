<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Search Result</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bottstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css">
  </head>

  <body>
    <div class="container mt-4">
      <div class = "card>
        <div class="card-body">
	  <%
            if(sampleData.length > 0)
            {
                sampleData.forEach(function(data){
            %>
		<h2> <%= data.Name%> </h2>
		<p>Symptoms: <%= data.Symptoms %></p>
                <p>Treatments: <%= data.Treatments %></p>
            <%
                });
            } else {
            %>
            <h6> No Data Found </h6>
            <%
             }
           %>
        </div>
      </div>
    </div>
  </body>
</html>
